<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:p="clr-namespace:Tegu.Net.Client.Maui.Pages"
             x:Class="Tegu.Net.Client.Maui.Pages.MainPage"
             x:DataType="p:MainVM"
             Title="Main">
			 
    <StackLayout HorizontalOptions="Center">

        <Button Margin="10" Text="Go To Controls" Command="{Binding NavigateToControlsCommand}"/>
        <Button Margin="10" Text="Go To MVVM" Command="{Binding NavigateToMVVMCommand}"/>

    </StackLayout>
 
</ContentPage>
